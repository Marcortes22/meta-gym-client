{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "new-york",
  "rsc": true,
  "tsx": true,
  "tailwind": {
    "config": "",
    "css": "app/globals.css",
    "baseColor": "neutral",
    "cssVariables": true,
    "prefix": ""
  },
  "iconLibrary": "lucide",
  "aliases": {
    "components": "@/shared/components",
    "utils": "@/lib/utils",
    "ui": "@/shared/components/ui",
    "lib": "@/lib",
    "hooks": "@/shared/hooks"
  },
  "registries": {
    "@animate-ui": {
      "url": "https://animate-ui.com/r/{name}.json",
      "aliases": {
        "components": "@/shared/components",
        "primitives": "@/shared/components/primitives",
        "radix": "@/shared/components/ui",
        "utils": "@/lib/utils"
      }
    }
  }
}